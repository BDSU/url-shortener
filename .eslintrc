{
	"env": {
		"commonjs": true,
		"es2021": true,
		"browser": true,
		"jest": true
	},
  	"root": true,
  	"extends": "eslint:recommended",
  	"rules": { 
		"no-console": 1,
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		]
	}  
}